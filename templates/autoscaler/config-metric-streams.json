{% for metric in metrics %}
{
    "id": "{{ metric.name }}",
    "database": "{{ metric.database }}",
    "measurement": "{{ metric.tag }}",
    "fieldKey": "{{ metric.field }}",
    "function": "{{ metric.aggregate_function }}",
    "downsample": {
        "time": {{ metric.downsample }},
        "unit": "seconds"
    },
    "dataSettlingTime":  {
        "time": {{ metric.data_settling }},
        "unit": "seconds"
    }
}{% if not loop.last %},{% endif %}
{% endfor %}
