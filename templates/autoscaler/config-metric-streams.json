{% for metric in metrics %}
{
    "id": "{{ metric.name }}",
    "database": "{{ metric.database }}",
    "query": {
        "select": "{{ metric.aggregate_function }}({{ metric.field }})",
        "from": "{{ metric.tag }}",
        "groupBy": "time({{ metric.downsample }}s)"
    },
    "dataSettlingTime":  {
        "time": {{ metric.data_settling }},
        "unit": "seconds"
    }
}{% if not loop.last %},{% endif %}
{% endfor %}
